<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<!--数据库驱动jar -->
	<classPathEntry location="/Users/lzf/delevelop/mylib/mysql/mysql-connector-java/5.1.40/mysql-connector-java-5.1.40.jar" />
	<context id="MySql" targetRuntime="MyBatis3">
		<!--去除注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="mergeable" value="false"></property>
		</commentGenerator>
		
		<!--数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://192.168.1.50:3306/zktx_data_platform?" userId="root" password="passwd"/>
		
		<!--生成实体类 指定包名 以及生成的地址 （可以自定义地址，但是路径不存在不会自动创建 使用Maven生成在target目录下，会自动创建） -->
		<javaModelGenerator targetPackage="com.zktx.platform.entity.tb" targetProject="platform"/>
		
		<!--生成SQLMAP文件 -->
		<sqlMapGenerator targetPackage="com.zktx.platform.mapping.tb" targetProject="platform"/>
		
		<!--生成Dao文件 可以配置 type="XMLMAPPER"生成xml的dao实现 context id="DB2Tables" 修改targetRuntime="MyBatis3" -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.zktx.platform.dao.tb" targetProject="platform"/>

		<!--对应数据库表 mysql可以加入主键自增 字段命名 忽略某字段等 -->
		<table tableName="tb_task_history" domainObjectName="TaskHistory" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false">
			<property name="useActualColumnNames" value="true" />
		</table>
		
	</context>
</generatorConfiguration>